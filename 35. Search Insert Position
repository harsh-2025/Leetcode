class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
        int x=0,y=0;
        int n=nums.size();
        for(int i=0;i<n;i++){
            if(nums[i]==target){
                x=i;
            }
            else if(nums[i]<target){
                y++;
            }
        }
        return x,y;

    }
};
